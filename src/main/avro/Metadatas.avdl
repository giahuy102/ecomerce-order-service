@namespace("com.ecomerce.ms.service")
protocol Metadatas {
	record Metadata {
		@logicalType("timestamp_ms")
		long submitTime;
	}

	record SagaMetadata {
		@logicalType("uuid")
		string instanceId;

		string stepKey;
	}

	enum SagaStepStatus {
		SUCCEEDED, FAILED // Other states are managed by orchestration
	}
}